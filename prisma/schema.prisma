// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Watchlist {
  id           Int     @id @default(autoincrement())
  endpoint     String
  user_email   String
  comic_image  String?
  comic_titles String?

  @@unique([user_email, endpoint])
}
